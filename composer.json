{
  "name": "webpane/webpane-wordpress-plugin",
  "version": "0.0.1",
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "Webpane\\WebpaneWordpressPlugin\\": "src/",
      "Webpane\\WebpaneWordpressPlugin\\Tests\\": "tests/"
    }
  },
  "require": {
    "javanile/granular": "*",
    "symfony/yaml": "^4.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5.3",
    "composer/installers": "~1.0",
    "wpackagist-plugin/loco-translate":"2.3.0"
  },
  "extra": {
    "installer-paths": {
      "wp-content/plugins/{$name}/": ["type:wordpress-plugin"]
    }
  }
}
